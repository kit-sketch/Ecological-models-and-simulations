import matplotlib.pyplot as plt
import numpy as np

#def type_1(N,a):
    #return a*N

def feedtype2 (N, a, h):
    number = [N]
    feedrate = []
    for i in range(1,100):
        Nnow = number[i-1]
        if Nnow  <= 0:
            break
        feedratenow = (a * Nnow) / (1 + a * h * Nnow) 
    
        feedrate.append(feedratenow)
        number.append(Nnow-feedratenow)
    return number, feedrate

def feedtype3 (N,a,h):
    number = [N]
    feedrate = []
    for i in range(1,100):
        Nnow = number[i-1]
    
        if Nnow  <= 0:
            break
        feedratenow = (a * Nnow**2) / (1 + a * h * Nnow**2) 
    
        feedrate.append(feedratenow)
        number.append(Nnow-feedratenow)
    return number, feedrate    

Nin = 50
a= 0.1
h= 0.05

plt.figure(figsize = (8,5))
Number1, feedrate1 = feedtype2(Nin, a, h)
Number2, feedrate2 = feedtype3(Nin, a, h)
plt.plot(feedrate1, label = "type 2")
plt.plot(feedrate2, label = "type 3")
plt.xlabel("time ")
plt.ylabel("Feeding rate")
plt.title("Feeding rate for non replenishing prey density")
plt.legend()
plt.show()

plt.figure(figsize = (8,5))
plt.plot(Number1, label = "type 2")
plt.plot(Number2, label = "type 3")
plt.xlabel("time ")
plt.ylabel("prey density")
plt.title("Prey density for non replenishing prey density")
plt.legend()
plt.show()